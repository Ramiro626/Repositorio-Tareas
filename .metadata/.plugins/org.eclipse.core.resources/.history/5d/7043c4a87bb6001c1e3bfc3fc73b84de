#include <stdio.h>
#include <stdlib.h>

void sum(int a, int b);

int main(void) {
	setbuf(stdout, NULL);
    int a, b;

    printf("Enter a:\n");
    scanf("%d", &a);
    printf("enter b\n");
    scanf("%d", &b);

    sum(a,b);

    return 0;
}

void sum(int a, int b) {
	int c, d, ola;
	ola = 0;
	c = a;
	d = b;
    ola = c + d;
    printf("sumar es %d", ola);
}

